<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="expandable_selector">
    <attr name="hide_background_if_collapsed" format="boolean"/>
    <attr name="animation_duration" format="integer"/>
    <attr name="hide_first_item_on_collapse" format="boolean"/>
  </declare-styleable>
  <attr name="position" format="enum">
    <enum name="left_top" value="1" />
    <enum name="right_top" value="2" />
    <enum name="left_bottom" value="4" />
    <enum name="right_bottom" value="8" />
  </attr>
  <!--半径 -->
  <attr name="radius" format="dimension"/>
  <declare-styleable name="SateliteMenu">
    <attr name="position" />
    <attr name="radius" />
  </declare-styleable>
  <attr name="expandableItemStyle"/>

</resources>